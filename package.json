{
  "name": "vscode-extension-bade",
  "displayName": "BADE (Bourne Again DEploy)",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.58.0"
  },
  "activationEvents": [
    "onCommand:extension.compareFiles", "onCommand:extension.deployFile"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.compareFiles",
        "title": "Bade: Compare Files"
      },
      {
        "command": "extension.deployFile",
        "title": "Bade: Deploy File"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.6.2",
    "ssh2": "^1.15.0",
    "vsce": "^2.15.0"
  },
  "scripts": {
    "pack": "vsce package"
  },
  "repository": {
  }
}
